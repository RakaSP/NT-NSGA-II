{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Run Runner.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/Runner.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
